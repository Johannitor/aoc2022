{
  "compilerOptions": {
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "es6",
    "removeComments": true,
    "strict": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "lib": ["esnext", "DOM"],
    "esModuleInterop": true,
    "module": "CommonJS",
    "outDir": "dist"
  },
  "types": ["node"],
  "exclude": ["node_modules"],
  "include": ["**/*.ts", "../shared/lib/AbstractDoor.ts"]
}
